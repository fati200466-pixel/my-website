            onDataChanged(data) {
                // Update UI if needed
            }
        };

        // Initialize Data SDK
        async function initDataSDK() {
            if (window.dataSdk) {
                const result = await window.dataSdk.init(dataHandler);
                if (!result.isOk) {
                    console.error('ÙØ´Ù„ ØªÙ‡ÙŠØ¦Ø© Ù†Ø¸Ø§Ù… Ø­ÙØ¸ Ø§Ù„Ø¨ÙŠØ§Ù†Ø§Øª');
                }
            }
        }

