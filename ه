            currentStudent.grade = gradeLevel;
            
            document.getElementById('registrationPage').classList.add('hidden');
            document.getElementById('subjectsPage').classList.remove('hidden');
        });

        // Start Quiz Function
        function startQuiz(subject, modelNumber) {
            const subjectNames = {
                'arabic': 'Ø§Ù„Ù„ØºØ© Ø§Ù„Ø¹Ø±Ø¨ÙŠØ©',
                'math': 'Ø§Ù„Ø±ÙŠØ§Ø¶ÙŠØ§Øª',
                'science': 'Ø§Ù„Ø¹Ù„ÙˆÙ…'
