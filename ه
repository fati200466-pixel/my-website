        // SDK Initialization
        async function onConfigChange(config) {
            document.getElementById('platformTitle').textContent = config.platform_title || defaultConfig.platform_title;
            document.getElementById('schoolName').textContent = config.school_name || defaultConfig.school_name;
            document.getElementById('teacherName').textContent = config.teacher_name || defaultConfig.teacher_name;
            document.getElementById('principalName').textContent = config.principal_name || defaultConfig.principal_name;
            
            
        }

        // Initialize Element SDK
        if (window.elementSdk) {
            window.elementSdk.init({
                defaultConfig: defaultConfig,
                onConfigChange: onConfigChange,
                mapToCapabilities: (config) => ({
                    recolorables: [],
                    borderables: [],
                    fontEditable: undefined,
                    fontSizeable: undefined
                }),
                mapToEditPanelValues: (config) => new Map([
                    ['platform_title', config.platform_title || defaultConfig.platform_title],
                    ['school_name', config.school_name || defaultConfig.school_name],
                    ['teacher_name', config.teacher_name || defaultConfig.teacher_name],
                    ['principal_name', config.principal_name || defaultConfig.principal_name],
                    
                ])
            });
        }

        // Data SDK Handler
